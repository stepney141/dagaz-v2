  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "eslintIgnore": [
    ".eslintrc.json"
  ],
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "benny": "^3.7.1",
    "esbuild": "^0.14.48",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsdoc": "^39.3.25",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "fs": "*",
    "glob": "^7.2.3",
    "jest": "^28.1.1",
    "node-qunit-puppeteer": "^2.1.0",
    "qunitjs": "^1.23.1",
    "ts-jest": "^28.0.4",
    "ts-migrate": "^0.1.29",
    "typescript": "^4.7.3"
  },
  "scripts": {
    "prepare-test": "mkdir ./coverage && mkdir ./coverage/old && mkdir ./coverage/refactored",
    "test-zrf-old": "node qunit-test-runner.js zrf-based-kernel old",
    "test-zrf-refactored": "node qunit-test-runner.js zrf-based-kernel refactored",
    "test-new-model-old": "node qunit-test-runner.js new-model-kernel old",
    "test-new-model-refactored": "node qunit-test-runner.js new-model-kernel refactored",
    "test-v2": "node qunit-test-runner.js v2",
    "test": "jest",
    "test-esm": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:coverage": "npm run test -- --coverage",
    "test:coverage-ci": "npm run test -- --coverage --silent --testLocationInResults --ci --json --outputFile=\"report.json\"",
    "test:type-check": "tsc --pretty --noEmit",
    "lint": "eslint .",
    "build": "tsc",
    "exec": "node -r esbuild-register",
    "exec:babel": "babel-node --extensions .ts,.js --presets @babel/preset-env,@babel/preset-typescript"
  },
  "dependencies": {
    "underscore": "^1.13.4"
  }
}
