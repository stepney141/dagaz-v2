{
  "name": "dagaz-game",
  "version": "2.0.0-alpha",
  "description": "",
  "license": "MIT License",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "eslintIgnore": [
    ".eslintrc.json"
  ],
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/node": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.11",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "benny": "^3.7.1",
    "esbuild": "^0.16.2",
    "esbuild-register": "^3.4.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "fs": "*",
    "glob": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-migrate": "^0.1.35",
    "typedoc": "^0.23.21",
    "typedoc-theme-hierarchy": "^3.0.0",
    "typescript": "^4.9.4"
  },
  "scripts": {
		"type-check": "tsc --pretty --noEmit",
		"type-check:watch": "tsc --pretty --noEmit --watch",
		"test": "npm run type-check && jest",
    "test:jest": "jest",
		"test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage-ci": "jest --coverage --silent --testLocationInResults --ci --json --outputFile=\"report.json\"",
    "lint-fix": "prettier --write ./src ./tests ./benchmark ./games  && eslint ./src ./tests ./benchmark ./games --fix",
    "build": "tsc",
    "exec": "node -r esbuild-register",
    "exec:babel": "babel-node --extensions .ts,.js --presets @babel/preset-env,@babel/preset-typescript",
    "docs": "typedoc --options ./typedoc.json"
  },
  "dependencies": {
    "underscore": "^1.13.6"
  }
}
